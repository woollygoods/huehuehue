{
    "name": "huehuehue",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev:client": "vite",
        "build:client": "vite build",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "bindings:debug": "cd src-tauri && cargo run -- --generate-bindings-only",
